							"typeProperties": {
								"scripts": [
									{
										"type": "Query",
										"text": {
											"value": "DELETE FROM dbo.DateTime\nWHERE Dates = @{item().Dates};",
											"type": "Expression"
										}
									}
								],
								"scriptBlockExecutionTimeout": "02:00:00"
							}
						},
						{
							"name": "tempIterationCount",
							"type": "SetVariable",
