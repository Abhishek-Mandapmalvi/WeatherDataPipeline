								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "iterationCount",
								"value": {
									"value": "@variables('tempIterationCount')",
									"type": "Expression"
								}
							}
						},
						{
							"name": "If Condition1",
							"type": "IfCondition",
							"dependsOn": [
								{
									"activity": "Iteration",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"expression": {
									"value": "@equals(variables('iterationCount'), 1000)",
									"type": "Expression"
								},
								"ifFalseActivities": [
									{
										"name": "Stop",
										"type": "Fail",
										"dependsOn": [],
